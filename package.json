{
  "name": "wechat4u",
  "version": "0.2.6",
  "description": "web wechat lib",
  "main": "lib/wechat.js",
  "scripts": {
    "example": "webpack --config=./example/webpack.config.js && cross-env DEBUG=wechat,wxbot,app node ./example/app.js",
    "compile": "babel src -d lib -s",
    "prepublish": "npm run compile",
    "lint": "eslint src",
    "clean": "rm -rf lib"
  },
  "dependencies": {
    "axios": "^0.9.1",
    "cookie-manager": "0.0.16",
    "debug": "^2.2.0",
    "form-data": "^1.0.0-rc4",
    "mime": "^1.3.4"
  },
  "devDependencies": {
    "babel-cli": "^6.6.0",
    "babel-core": "^6.0.0",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-2": "^6.5.0",
    "css-loader": "^0.23.0",
    "es6-promise": "^3.1.2",
    "eslint": "^2.2.0",
    "estraverse-fb": "^1.3.1",
    "express": "^4.13.4",
    "jade": "^1.11.0",
    "json-loader": "^0.5.4",
    "stylus-loader": "^1.4.0",
    "url-loader": "^0.5.7",
    "vue": "^1.0.16",
    "vue-hot-reload-api": "^1.2.0",
    "vue-html-loader": "^1.0.0",
    "vue-loader": "^8.0.0",
    "vue-router": "^0.7.11",
    "vue-strap": "^1.0.7",
    "vue-style-loader": "^1.0.0",
    "webpack": "^1.12.2",
    "cross-env": "^1.0.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nodeWechat/wechat4u.git"
  },
  "keywords": [
    "wechat",
    "weixin"
  ],
  "author": "nodeWechat",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nodeWechat/wechat4u/issues"
  },
  "homepage": "https://github.com/nodeWechat/wechat4u#readme",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "rules": {
      "camelcase": 2,
      "curly": 2,
      "brace-style": [2, "1tbs"],
      "quotes": [2, "single"],
      "space-infix-ops": 2
    },
    "env": {
      "browser": true,
      "node": true
    }
  }
}
